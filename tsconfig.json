{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021","DOM","DOM.Iterable"],
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": { "@/*": ["src/*"] },
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["next-env.d.ts","src/**/*"],
  "exclude": ["node_modules"]
}
